{
  "name": "mingyi-portfolio",
  "version": "1.0.0",
  "description": "Mingyi Zou Art Portfolio with Payment System",
  "main": "index.html",
  "scripts": {
    "dev": "python -m http.server 8000",
    "dev:local": "node server.js",
    "dev:cloudflare": "wrangler pages dev .",
    "server": "node server.js",
    "start": "node server.js",
    "optimize-images": "node optimize-images.js",
    "update-data": "node update-data-with-optimized.js",
    "build": "npm run optimize-images && npm run update-data && ./prepare-deploy.sh",
    "deploy": "wrangler pages deploy .",
    "deploy:preview": "wrangler pages deploy . --branch main"
  },
  "dependencies": {
    "stripe": "^14.0.0",
    "express": "^4.18.2",
    "sharp": "^0.33.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "wrangler": "^3.19.0"
  },
  "keywords": ["art", "portfolio", "stripe", "payment"],
  "author": "Mingyi Zou",
  "license": "MIT"
}
